package models

import (
    "time"

    "github.com/google/uuid"
    "gorm.io/gorm"
)

type {{ .Name }} struct {
    ID   uuid.UUID `gorm:"type:uuid;primaryKey;"`
    Name string    `gorm:"type:varchar(255);not null"`

    CreatedAt time.Time
	UpdatedAt time.Time
	DeletedAt gorm.DeletedAt `gorm:"index"`
}
